<?xml version="1" encoding="UTF-8" ?>
<aiml>

	<category>
		<pattern>ASK WIKIPEDIA * </pattern>
		<template>
            <think><set name="topic">ASKWIKIPEDIA</set></think>
            <srai>YASKWIKIPEDIA <star /></srai>
        </template>
	</category>

    <topic name="ASKWIKIPEDIA">
        <category>
            <pattern>YASKWIKIPEDIA *</pattern>
            <template>
                <srai>ASKWIKIPEDIA <star /></srai>
            </template>
        </category>

        <category>
            <pattern>YASKWIKIPEDIA NULL</pattern>
            <!-- Simulate a null response -->
            <template>
            </template>
        </category>

        <category>
            <pattern>YEMPTY</pattern>
            <template>
                <think><set name="topic"></set></think>
                Nothing from Wikipedia
            </template>
        </category>
    </topic>

    <category>
        <pattern>YEMPTY</pattern>
        <template>Nothing, absolutely nothing!</template>
    </category>

</aiml>